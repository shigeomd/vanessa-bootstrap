	 ИЛИ ВыбТабличноеПолеИмя = "Раздел9"
	 ИЛИ ВыбТабличноеПолеИмя = "Раздел9_Прил1"
	 ИЛИ ВыбТабличноеПолеИмя = "Раздел10"
	 ИЛИ ВыбТабличноеПолеИмя = "Раздел11"
