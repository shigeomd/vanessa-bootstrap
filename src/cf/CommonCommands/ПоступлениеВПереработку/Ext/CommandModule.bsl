
&НаКлиенте
Процедура ОбработкаКоманды(ПараметрКоманды, ПараметрыВыполненияКоманды)

	КлючеваяОперация = "СозданиеФормыСпискаПоступлениеТоваровУслуг";
	ОценкаПроизводительностиКлиентСервер.НачатьЗамерВремени(КлючеваяОперация);

	Отбор = Новый Структура("ВидОперации", ПредопределенноеЗначение("Перечисление.ВидыОперацийПоступлениеТоваровУслуг.ВПереработку"));
	ПараметрыФормы = Новый Структура();
	ПараметрыФормы.Вставить("Отбор", 							Отбор);
	ПараметрыФормы.Вставить("НеИзменятьОтборПоВидуОперации", 	Истина);
	ПараметрыФормы.Вставить("Заголовок", 						НСтр("ru = 'Поступление в переработку'"));
	ОткрытьФорму("Документ.ПоступлениеТоваровУслуг.ФормаСписка", ПараметрыФормы, ПараметрыВыполненияКоманды.Источник, "ФормаСписка_ПоступлениеВПереработку", ПараметрыВыполненияКоманды.Окно);
КонецПроцедуры
