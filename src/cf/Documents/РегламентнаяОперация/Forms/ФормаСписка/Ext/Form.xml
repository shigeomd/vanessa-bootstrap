	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>Список.Ref</Field>
				<Field>Список.Состояние</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументРО.Ссылка,
	ДокументРО.ПометкаУдаления,
	ДокументРО.Номер,
	ДокументРО.Дата,
	ДокументРО.Проведен,
	ДокументРО.Организация,
	ДокументРО.ВидОперации,
	ВЫБОР
		КОГДА ДокументРО.ПометкаУдаления
			ТОГДА 2
		КОГДА ДокументРО.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.НеВыполнено)
			ТОГДА 0
		КОГДА ДокументРО.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.ВыполненоСошибками)
			ТОГДА 6
		КОГДА ДокументРО.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.Выполнено)
				И ДокументРО.РучнаяКорректировка
			ТОГДА 5
		КОГДА ДокументРО.Состояние = ЗНАЧЕНИЕ(Перечисление.ВидыСостоянийРегламентныхОпераций.Пропущено)
			ТОГДА 7
		ИНАЧЕ 4
	КОНЕЦ КАК СостояниеДокумента,
	ДокументРО.Комментарий,
	ДокументРО.РучнаяКорректировка,
	ДокументРО.Состояние,
	ДокументРО.МоментВремени,
	ДокументРО.Ответственный
ИЗ
	Документ.РегламентнаяОперация КАК ДокументРО</QueryText>
				<MainTable>Document.РегламентнаяОперация</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:item xsi:type="dcsset:FilterItemComparison">
							<dcsset:use>false</dcsset:use>
							<dcsset:left xsi:type="dcscor:Field">ВидОперации</dcsset:left>
							<dcsset:comparisonType>Equal</dcsset:comparisonType>
							<dcsset:userSettingID>02670c85-709a-4dd7-943f-2bf95ba05092</dcsset:userSettingID>
						</dcsset:item>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
