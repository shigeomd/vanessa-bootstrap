	СтрокаДоходов.П000060001505 = Окр(СтрокаДоходов.П000060001505,0); // по второй месяц отчетного периода
	СтрокаДоходов.П000060001305 = СтрокаДоходов.П000060001405 - СтрокаДоходов.П000060001305;
	СтрокаДоходов.П000060001405 = СтрокаДоходов.П000060001505 - СтрокаДоходов.П000060001405;
	СтрокаДоходов.П000060001505 = СтрокаДоходов.П000060001105 - СтрокаДоходов.П000060001505;
	
	ЗаполнитьЗначенияСвойств(Контейнер.Раздел1_3, СтрокаДоходов);
	ЗаполнитьЗначенияСвойств(Контейнер.Раздел2_1, СтрокаДоходов);
	
	// Раздел I (табл 3.1)
	
	Запрос.Текст = ДоходыИнвалидовТекст;
	Результат = Запрос.Выполнить();
	
	ЧисленностьИнвалидов = 0;
	
	Если Не Результат.Пустой() Тогда // не было инвалидов совсем
		
		Запрос.Текст = ПроверкаТекст;
		Выборка = Запрос.Выполнить().Выбрать();
		// Раздел 3.1 НЕ выводим для льготных тарифов, контрольное соотношение №6 по разделу "Таблица 3.1 «Сведения,
		// необходимые для применения плательщиками страховых взносов пониженного тарифа страховых взносов, установленного
		// частью 2 статьи 58 Федерального закона от 24 июля 2009 г. № 212-ФЗ»".
		Если Выборка.Следующий() И Не Выборка.ВыводитьРаздел3_1 Тогда
			// Контрольное соотношение №7 по разделу "Информационные показатели титульного листа".
			ЧисленностьИнвалидов = Результат.Выбрать().Количество();
		Иначе 	
			ВременнаяТаблица = Результат.Выгрузить();
			ВременнаяТаблица.Индексы.Добавить("ФизическоеЛицо");
			// Контрольное соотношение №5 по разделу "Таблица 3.1 «Сведения, необходимые для применения плательщиками страховых
			// взносов пониженного тарифа страховых взносов, установленного частью 2 статьи 58 Федерального закона от 24 июля
			// 2009 г. № 212-ФЗ»"
			ЧисленностьИнвалидов = ВременнаяТаблица.Количество();
			// дополнительная обработка в целях округления.
			// за весь отчетный период
			ЗарплатаКадры.ОкруглитьСлагаемыеПоСумме(ВременнаяТаблица,"П000310001005", Окр(СтрокаДоходов._П000030005003,0));
			// за предыдущий отчетный период
			ЗарплатаКадры.ОкруглитьСлагаемыеПоСумме(ВременнаяТаблица,"П000310001006", Окр(СтрокаДоходов._П000030005004,0));
			// по первый месяц отчетного периода
			ЗарплатаКадры.ОкруглитьСлагаемыеПоСумме(ВременнаяТаблица,"П000310001007", Окр(СтрокаДоходов._П000030005005,0));
			// по второй месяц отчетного периода
			ЗарплатаКадры.ОкруглитьСлагаемыеПоСумме(ВременнаяТаблица,"П000310001008", Окр(СтрокаДоходов._П000030005006,0));
			
			НомерСтроки = 1;
			Для каждого СтрокаДанных Из ВременнаяТаблица Цикл
				СтрокаДанных.П000310001006 = СтрокаДанных.П000310001007 - СтрокаДанных.П000310001006;
				СтрокаДанных.П000310001007 = СтрокаДанных.П000310001008 - СтрокаДанных.П000310001007;
