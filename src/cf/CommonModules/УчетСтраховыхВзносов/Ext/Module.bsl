	КонецЕсли;
	
	Запрос.Текст = КонечныйОстатокТекст;
	Выборка = Запрос.Выполнить().Выбрать();
	Если Выборка.Следующий() Тогда
		ЗаполнитьЗначенияСвойств(Контейнер.Раздел2_1, Выборка);
	КонецЕсли;
	
