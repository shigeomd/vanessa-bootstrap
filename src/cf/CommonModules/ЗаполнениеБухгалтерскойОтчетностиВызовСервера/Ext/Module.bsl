Функция ПорядокРазверткиСчетов(СписокСчетов)
	
	СтруктураРазвертки = Новый Структура;
	
	СтруктураРазвертки.Вставить("СчетаПоСрочности", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПоСрочностиРазвернуто", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПоНДСАвансов", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПлатежейРазвернуто", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПлатежейПоВидамПлатежей", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПлатежейПоВидамПлатежейУровням", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаКредитовЗаймовБезРазвертки", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаКредитовЗаймовПеревод", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаБезРазвертки", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПоРаботникамРазвернуто", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПоКонтрагентамРазвернуто", Новый Массив);
	СтруктураРазвертки.Вставить("СчетаПоКонтрагентамДоговорам", Новый Массив);
	
	ПромежуточныйРезультат = Новый Соответствие;
	
	СпособыРазверткиСчетов = СпособыРазверткиСчетовПояснений5();
	
	Счета = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок(СписокСчетов);
	Для Каждого КодСчета Из Счета Цикл
		Если НЕ ЗначениеЗаполнено(КодСчета) Тогда
			Продолжить;
		КонецЕсли;
		
		КодСчета = СокрЛП(КодСчета);
		СпособРазвертки = СпособыРазверткиСчетов[КодСчета];
		Если СпособРазвертки = Неопределено Тогда
			ВызватьИсключение "Для счета с условным кодом " + КодСчета + " не указан способ развертки";
			Продолжить;
		КонецЕсли;
		
		ПромежуточныйРезультат.Вставить(КодСчета, СпособРазвертки);
		
	КонецЦикла;
	
	Для Каждого СпособРазверткиСчета Из ПромежуточныйРезультат Цикл
		КодСчета = СпособРазверткиСчета.Ключ;
		СпособРазвертки = СпособРазверткиСчета.Значение;
		
		СтруктураРазвертки[СпособРазвертки].Добавить(Счет(КодСчета));
	КонецЦикла;
	
	Возврат СтруктураРазвертки;
	
КонецФункции

