	СКК_76_ВА = СКК("76.ВА", БухИтоги);
	
	ЗначениеПоказателя = ?(СтрокаОтраженияСчета76_ВА = "1550", СКК_76_ВА, 0) + СККР_76_НА_КонтрагентыДоговоры;
	УстановитьЗначениеПоказателя(РассчитанныеПоказатели, ИмяРаздела, ИмяОбластиПоказателя, ЗначениеПоказателя);
	
	Если СтрокаОтраженияСчета76_ВА = "1550" Тогда
		ДобавитьСтрокуРасшифровки(ТаблицаРасшифровки, ИмяОбластиПоказателя, ЗаголовокРасшифровки, "+", "!СКК " + Счет("76.ВА").Код, СКК_76_ВА, ИмяРаздела, НастройкиОСВПоСчету(ОбщиеПараметрыРасшифровки, Счет("76.ВА")));
		ДобавитьСтрокуРасшифровки(ТаблицаРасшифровки, ИмяОбластиПоказателя, , "+", "Сальдо на конец периода по кредиту счета 76.НА, развернутое по субконто ""Контрагенты"", ""Договоры""", СККР_76_НА_КонтрагентыДоговоры, ИмяРаздела, НастройкиОСВПоСчетуКонтрагентыДоговорыРазвернуто(ОбщиеПараметрыРасшифровки, Счет("76.НА")));
	Иначе
		ДобавитьСтрокуРасшифровки(ТаблицаРасшифровки, ИмяОбластиПоказателя, ЗаголовокРасшифровки, "+", "Сальдо на конец периода по кредиту счета 76.НА, развернутое по субконто ""Контрагенты"", ""Договоры""", СККР_76_НА_КонтрагентыДоговоры, ИмяРаздела, НастройкиОСВПоСчетуКонтрагентыДоговорыРазвернуто(ОбщиеПараметрыРасшифровки, Счет("76.НА")));
	КонецЕсли;
	
КонецПроцедуры

Процедура ЗаполнитьФормаОтчета2011Кв4_ОФР(КонтекстИсполнения)
	
	Организация = КонтекстИсполнения.Организация;
	ТаблицаРасшифровки = КонтекстИсполнения.ТаблицаРасшифровки;
	ИспользуютсяМакетыЕНВД = КонтекстИсполнения.ПрименениеЕНВД;
	РассчитанныеПоказатели = КонтекстИсполнения.РассчитанныеПоказатели;
	
	ИмяРаздела = "ФормаОтчета";
	
	НачалоПериодаОтчета = КонтекстИсполнения.НачалоПериодаОтчета;
	КонецПериодаОтчета  = КонтекстИсполнения.КонецПериодаОтчета;
	
	НачалоПериодаПредыдущегоГода = ДобавитьМесяц(НачалоПериодаОтчета, -12);
	КонецПериодаПредыдущегоГода  = ДобавитьМесяц(КонецПериодаОтчета, -12);
	
	Если НачалоМесяца(КонецПериодаОтчета) = НачалоПериодаОтчета Тогда
		// Описание периода одного месяца.
		ОписаниеПериода = Формат(КонецПериодаОтчета, "ДФ = 'ММММ гггг'") + "г.";
		ОписаниеПериодаПрошлогоГода = Формат(КонецПериодаПредыдущегоГода, "ДФ = 'ММММ гггг'") + "г.";
	Иначе
		// Описание диапазона месяцев.
		ОписаниеПериода = Формат(НачалоПериодаОтчета, "ДФ = 'ММММ гггг'") + " - " + Формат(КонецПериодаОтчета, "ДФ = 'ММММ гггг'") + "г.";
		ОписаниеПериодаПрошлогоГода = Формат(НачалоПериодаПредыдущегоГода, "ДФ = 'ММММ гггг'") + " - " + Формат(КонецПериодаПредыдущегоГода, "ДФ = 'ММММ гггг'") + "г.";
	КонецЕсли;
	
	ЗаголовокДатаОтчетногоПериода = "За " + ОписаниеПериода;
	ЗаголовокДатаОтчетногоПериодаПрошлогоГода = "За " + ОписаниеПериодаПрошлогоГода;
	
	// Вспомогательные даты периодов.
	ДатаНачалаПериодаОтчета = НачалоПериодаОтчета;
	ДатаКонцаПериодаОтчета = КонецПериодаОтчета;
	ДатаНачалаПериодаПредшествующегоГода = ДобавитьМесяц(НачалоПериодаОтчета, -12);
	ДатаКонцаПериодаПредшествующегоГода = ДобавитьМесяц(КонецПериодаОтчета, -12);
	
	ЗаполнитьФормаОтчета2011Кв4_ОФР_ПоГрафе("04", ЗаголовокДатаОтчетногоПериода, КонтекстИсполнения, НачалоПериодаОтчета, КонецПериодаОтчета);
	
	ЗаполнятьПредыдущиеПериоды = НЕ РасширенныйПериодОтчета(ДатаНачалаПериодаОтчета);
	Если ЗаполнятьПредыдущиеПериоды Тогда
		ЗаполнитьФормаОтчета2011Кв4_ОФР_ПоГрафе("05", ЗаголовокДатаОтчетногоПериодаПрошлогоГода, КонтекстИсполнения, НачалоПериодаПредыдущегоГода, КонецПериодаПредыдущегоГода);
	КонецЕсли;
	
КонецПроцедуры

Процедура ЗаполнитьФормаОтчета2011Кв4_ОФР_ПоГрафе(Графа, ЗаголовокГрафы, КонтекстИсполнения, НачалоПериодаОтчета, КонецПериодаОтчета)
	
	Организация = КонтекстИсполнения.Организация;
	ТаблицаРасшифровки = КонтекстИсполнения.ТаблицаРасшифровки;
	ИспользуютсяМакетыЕНВД = КонтекстИсполнения.ПрименениеЕНВД;
	РассчитанныеПоказатели = КонтекстИсполнения.РассчитанныеПоказатели;
	
	ИмяРаздела = "ФормаОтчета";
	
	ОбщиеПараметрыРасшифровки = Новый Структура;
	ОбщиеПараметрыРасшифровки.Вставить("Организация", Организация);
	ОбщиеПараметрыРасшифровки.Вставить("НачалоПериода", НачалоПериодаОтчета);
	ОбщиеПараметрыРасшифровки.Вставить("КонецПериода", КонецПериодаОтчета);
	
	ДеревоНастроекСтрокРасшифровки = КонтекстИсполнения.СоставДополнительныхСтрокОПУ;
	
	ОрганизацияПрименяетПБУ18 = КонтекстИсполнения.ОрганизацияПрименяетПБУ18;
	
	БухИтоги = Обработки.БухгалтерскиеИтоги.Создать();
	
	Итоги = "НачальныйОстатокДт, НачальныйОстатокКт,
	        |ОборотДт, ОборотКт,
	        |КорОборотДт, КорОборотКт,
	        |КонечныйОстатокДт, КонечныйОстатокКт";
	БухИтоги.РассчитатьИтоги("Хозрасчетный", Итоги, "Сумма", "Счет, КорСчет", НачалоПериодаОтчета, КонецПериодаОтчета, , , , , , "Организация", Организация);
	
	// Счета для расчета оборотов по видам прочих доходов и расходов.
	СчетаОборотов = Новый Массив();
	СчетаОборотов.Добавить(Счет("91.01"));
	СчетаОборотов.Добавить(Счет("91.02"));
	
	СчетаОборотовКор = Новый Массив();
	СчетаОборотовКор.Добавить(Счет("01")); // счета 01, 02, 03, 04, 05 необходимы для расчета доходов и расходов, связанных с переоценкой внеоборотных активов
	СчетаОборотовКор.Добавить(Счет("02"));
	СчетаОборотовКор.Добавить(Счет("03"));
	СчетаОборотовКор.Добавить(Счет("04"));
	СчетаОборотовКор.Добавить(Счет("05"));
	СчетаОборотовКор.Добавить(Счет("68.02"));
	
	// Обороты по видам прочих доходов и расходов.
	КО_91_01_ВПДР = Новый Структура;
	ДО_91_02_ВПДР = Новый Структура;
	ДО_91_02__68_02_ВПДР = Новый Структура;
	
	КО_91_01__010304_ПереоценкаВнеоборотныхАктивов = 0;
	ДО_91_02__010304_ПереоценкаВнеоборотныхАктивов = 0;
	
	КО_91_01__0205_ПереоценкаВнеоборотныхАктивов = 0;
	ДО_91_02__0205_ПереоценкаВнеоборотныхАктивов = 0;
	
	БухИтогиРС = Обработки.БухгалтерскиеИтоги.Создать();
	ИтогиРС = "ОборотДт, ОборотКт, КорОборотДт, КорОборотКт";
	БухИтогиРС.РассчитатьИтоги("Хозрасчетный", ИтогиРС, "Сумма", "Счет, Субконто1, КорСчет", НачалоПериодаОтчета, КонецПериодаОтчета, , СчетаОборотов, ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ПрочиеДоходыИРасходы, СчетаОборотовКор, , "Организация", Организация);
	ОстаткиИОбороты = БухИтогиРС.ПолучитьОстаткиИОбороты();
	
	Для Каждого ЗначениеПеречисления Из Метаданные.Перечисления.ВидыПрочихДоходовИРасходов.ЗначенияПеречисления Цикл
		КО_91_01_ВПДР.Вставить(ЗначениеПеречисления.Имя, 0);
		ДО_91_02_ВПДР.Вставить(ЗначениеПеречисления.Имя, 0);
		ДО_91_02__68_02_ВПДР.Вставить(ЗначениеПеречисления.Имя, 0);
	КонецЦикла;
	
	ВыборкаПоСубконто = ОстаткиИОбороты.Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам, "Субконто1");
	
	Пока ВыборкаПоСубконто.Следующий() Цикл
		
		ВыбранноеСубконто = ВыборкаПоСубконто.Субконто1;
				
		Если ЗначениеЗаполнено(ВыбранноеСубконто) И ЗначениеЗаполнено(ВыбранноеСубконто.ВидПрочихДоходовИРасходов) Тогда
			ИндексВидаПДР = Перечисления.ВидыПрочихДоходовИРасходов.Индекс(ВыбранноеСубконто.ВидПрочихДоходовИРасходов);
			ИмяВидаПДР = Метаданные.Перечисления.ВидыПрочихДоходовИРасходов.ЗначенияПеречисления[ИндексВидаПДР].Имя;
			
			СуммаОБ = БухИтогиРС.ПолучитьИтог("СуммаОборотКт", "Счет, Субконто1", Счет("91.01"), ВыбранноеСубконто);
			КО_91_01_ВПДР[ИмяВидаПДР] = КО_91_01_ВПДР[ИмяВидаПДР] + СуммаОБ;
			
			СуммаОБ = БухИтогиРС.ПолучитьИтог("СуммаОборотДт", "Счет, Субконто1", Счет("91.02"), ВыбранноеСубконто);
			ДО_91_02_ВПДР[ИмяВидаПДР] = ДО_91_02_ВПДР[ИмяВидаПДР] + СуммаОБ;
			
			СуммаОБ = БухИтогиРС.ПолучитьИтог("СуммаКорОборотДт", "Счет, Субконто1, КорСчет", Счет("91.02"), ВыбранноеСубконто, Счет("68.02"));
			ДО_91_02__68_02_ВПДР[ИмяВидаПДР] = ДО_91_02__68_02_ВПДР[ИмяВидаПДР] + СуммаОБ;
			
			// Расчет показателей доходов и расходов, связанных с переоценкой внеоборотных активов.
			Если ВыбранноеСубконто.ВидПрочихДоходовИРасходов = Перечисления.ВидыПрочихДоходовИРасходов.ПереоценкаВнеоборотныхАктивов Тогда
				ДО_91_02__01 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотДт", "Счет, Субконто1, КорСчет", Счет("91.02"), ВыбранноеСубконто, Счет("01"));
				ДО_91_02__02 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотДт", "Счет, Субконто1, КорСчет", Счет("91.02"), ВыбранноеСубконто, Счет("02"));
				ДО_91_02__03 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотДт", "Счет, Субконто1, КорСчет", Счет("91.02"), ВыбранноеСубконто, Счет("03"));
				ДО_91_02__04 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотДт", "Счет, Субконто1, КорСчет", Счет("91.02"), ВыбранноеСубконто, Счет("04"));
				ДО_91_02__05 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотДт", "Счет, Субконто1, КорСчет", Счет("91.02"), ВыбранноеСубконто, Счет("05"));
				
				КО_91_01__01 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотКт", "Счет, Субконто1, КорСчет", Счет("91.01"), ВыбранноеСубконто, Счет("01"));
				КО_91_01__02 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотКт", "Счет, Субконто1, КорСчет", Счет("91.01"), ВыбранноеСубконто, Счет("02"));
				КО_91_01__03 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотКт", "Счет, Субконто1, КорСчет", Счет("91.01"), ВыбранноеСубконто, Счет("03"));
				КО_91_01__04 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотКт", "Счет, Субконто1, КорСчет", Счет("91.01"), ВыбранноеСубконто, Счет("04"));
				КО_91_01__05 = БухИтогиРС.ПолучитьИтог("СуммаКорОборотКт", "Счет, Субконто1, КорСчет", Счет("91.01"), ВыбранноеСубконто, Счет("05"));
				
				КО_91_01__010304_ПереоценкаВнеоборотныхАктивов = КО_91_01__010304_ПереоценкаВнеоборотныхАктивов
				                                               + КО_91_01__01 + КО_91_01__03 + КО_91_01__04;
				КО_91_01__0205_ПереоценкаВнеоборотныхАктивов   = КО_91_01__0205_ПереоценкаВнеоборотныхАктивов
				                                               + КО_91_01__02 + КО_91_01__05;
				ДО_91_02__010304_ПереоценкаВнеоборотныхАктивов = ДО_91_02__010304_ПереоценкаВнеоборотныхАктивов
				                                               + ДО_91_02__01 + ДО_91_02__03 + ДО_91_02__04;
				ДО_91_02__0205_ПереоценкаВнеоборотныхАктивов   = ДО_91_02__0205_ПереоценкаВнеоборотныхАктивов
				                                               + ДО_91_02__02 + ДО_91_02__05;
			КонецЕсли;
			
		ИначеЕсли НЕ ЗначениеЗаполнено(ВыбранноеСубконто) Тогда
			// Обороты, у которых не заполнен вид прочих доходов и расходов, учитываются как прочие внереализационные
			ВидПрочихДоходовИРасходов = Перечисления.ВидыПрочихДоходовИРасходов.ПрочиеВнереализационныеДоходыРасходы;
			ИндексВидаПДР = Перечисления.ВидыПрочихДоходовИРасходов.Индекс(ВидПрочихДоходовИРасходов);
			ИмяВидаПДР = Метаданные.Перечисления.ВидыПрочихДоходовИРасходов.ЗначенияПеречисления[ИндексВидаПДР].Имя;
			
			СуммаОБ = БухИтогиРС.ПолучитьИтог("СуммаОборотКт", "Счет, Субконто1", Счет("91.01"), ВыбранноеСубконто);
			КО_91_01_ВПДР[ИмяВидаПДР] = КО_91_01_ВПДР[ИмяВидаПДР] + СуммаОБ;
			
			СуммаОБ = БухИтогиРС.ПолучитьИтог("СуммаОборотДт", "Счет, Субконто1", Счет("91.02"), ВыбранноеСубконто);
			ДО_91_02_ВПДР[ИмяВидаПДР] = ДО_91_02_ВПДР[ИмяВидаПДР] + СуммаОБ;
			
			СуммаОБ = БухИтогиРС.ПолучитьИтог("СуммаКорОборотДт", "Счет, Субконто1, КорСчет", Счет("91.02"), ВыбранноеСубконто, Счет("68.02"));
			ДО_91_02__68_02_ВПДР[ИмяВидаПДР] = ДО_91_02__68_02_ВПДР[ИмяВидаПДР] + СуммаОБ;
		КонецЕсли;
		
	КонецЦикла;
	
	ИтогиРС = "ОборотДт, ОборотКт, КорОборотДт, КорОборотКт";
	БухИтогиРС.РассчитатьИтоги("Хозрасчетный", ИтогиРС, "Сумма", "Счет, Субконто1, КорСчет", НачалоПериодаОтчета, КонецПериодаОтчета, , Счет("99"), ПланыВидовХарактеристик.ВидыСубконтоХозрасчетные.ПрибылиИУбытки, , , "Организация", Организация);
	ДО_99_01_ЧрезвычайныеДоходыРасходы = БухИтогиРС.ПолучитьИтог("СуммаОборотДт", "Счет, Субконто1", Счет("99.01"), Перечисления.ПрибылиИУбытки.ЧрезвычайныеДоходыРасходы);
	КО_99_01_ЧрезвычайныеДоходыРасходы = БухИтогиРС.ПолучитьИтог("СуммаОборотКт", "Счет, Субконто1", Счет("99.01"), Перечисления.ПрибылиИУбытки.ЧрезвычайныеДоходыРасходы);
	ДО_99_01_НалогНаПрибыль            = БухИтогиРС.ПолучитьИтог("СуммаОборотДт", "Счет, Субконто1", Счет("99.01"), Перечисления.ПрибылиИУбытки.НалогНаПрибыль);
	ДО_99_01_НалоговыеСанкции          = БухИтогиРС.ПолучитьИтог("СуммаОборотДт", "Счет, Субконто1", Счет("99.01"), Перечисления.ПрибылиИУбытки.НалоговыеСанкции);
	
	НомерСтроки = "2110";
	НаименованиеСтроки = "Выручка";
	
	ИмяОбластиПоказателя = "П000100" + НомерСтроки + Графа;
	ЗаголовокРасшифровки = "Строка " + НомерСтроки + " """ + НаименованиеСтроки + """, графа """ + ЗаголовокГрафы + """";
	
	КО_90_01 = КО("90.01", БухИтоги);
	ДО_90_03 = ДО("90.03", БухИтоги);
	ДО_90_04 = ДО("90.04", БухИтоги);
	ДО_90_05 = ДО("90.05", БухИтоги);
	
